      actionType: 1
      coords: {x: 0, y: 0}
      moveSpeed: 0
    music: {fileID: 0}
    sound: {fileID: 0}
  - text: Yapo, como en los viejos tiempos. Voy a tu casa a las 5.
    audio: {fileID: 0}
    speaker: {fileID: 11400000, guid: d40a69069d73bac49b59f4162b5c784f, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 9d36779d286e1fc429050adbf0a956af, type: 2}
